<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.sales.core.Output" table="sales.OUTPUT">
        <id name="UID">
            <generator class="uuid"/>
        </id>
        <property column="paid" name="paid"/>
        <property column="paidPrice" name="paidPrice"/>
        <many-to-one class="com.warehouse.core.Input"
        name="input" column="input" />
    </class>
</hibernate-mapping>
